package variable;

public class Ex06 {
	public static void main(String[] args) {
		
		boolean bool1 = true;
		boolean bool2 = false;
		
		System.out.println(bool1);
		System.out.println(bool2);
		
		System.out.println(1 > 3);// 1이 3보다 크다? 거짓
		System.out.println(5 < 9);// 5는 9보다 작다? 참
		System.out.println(10 <= 10);	// 참
		// (비교연산자)는 boolean 값을 갖는다.
	}
	
}
