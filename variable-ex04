package variable;

public class Ex04 {
	public static void main(String[] args) {
		
		// 나이, 키, 몸무게를 변수로 선언하고 초기화해서 출력해보자
		
		int age;
		float height;
		double weight;
		
		age = 20;
		height = 167.2f;
		weight = 55.5;
		
		System.out.println(age);
		System.out.println(height);
		System.out.println(weight);
	}

}
